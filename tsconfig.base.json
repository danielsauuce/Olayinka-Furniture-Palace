{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "lib": ["DOM", "ESNext"],
      "strict": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noEmit": true
    }
  }